	style="-fx-background-color: yellow;"
	xmlns="http://javafx.com/javafx/null"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller08.Controller08">
	<columnConstraints>
		<ColumnConstraints minWidth="10.0" prefWidth="100.0" />
		<ColumnConstraints minWidth="10.0" prefWidth="100.0" />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints minHeight="10.0" prefHeight="30.0" />
		<RowConstraints minHeight="10.0" prefHeight="30.0" />
	</rowConstraints>
	<children>
		<Label alignment="CENTER" contentDisplay="CENTER"
			maxWidth="1.7976931348623157E308" text="画面8
			" GridPane.columnSpan="2" />
		<Button maxWidth="1.7976931348623157E308"
			mnemonicParsing="false" onAction="#handlePrev" text="戻る"
			GridPane.rowIndex="1" />
		<Button maxWidth="1.7976931348623157E308"
			mnemonicParsing="false" onAction="#handleNext" text="進む"
			GridPane.columnIndex="1" GridPane.rowIndex="1" />
